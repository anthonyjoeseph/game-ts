{
  "private": true,
  "name": "game-ts",
  "description": "Typesafe Game library for Typescript",
  "version": "0.7.0",
  "license": "MIT",
  "author": "Anthony Gabriele <typesafefrontend@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthonyjoeseph/game-ts"
  },
  "homepage": "https://github.com/anthonyjoeseph/game-ts",
  "bugs": "https://github.com/anthonyjoeseph/game-ts/issues",
  "keywords": [
    "functional-programming",
    "typescript",
    "fp-ts",
    "rxjs"
  ],
  "main": "./index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "tsc -p ./tsconfig.build.json -w",
    "build": "tsc -p ./tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "typecheck-dev": "tsc -w --noEmit",
    "lint": "eslint ./src/ ./test/ --ext ts",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check .",
    "test": "jest",
    "docs": "docs-ts",
    "prepub": "rm -rf dist && mkdir -p dist && cp ./README.md ./dist/ && cat ./package.json | grep -v '\"private\":' > ./dist/package.json && yarn run build"
  },
  "devDependencies": {
    "@types/eslint": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "docs-ts": "^0.5.2",
    "eslint": "^7.0.0",
    "eslint-plugin-functional": "^3.0.2",
    "fast-check": "^2.5.0",
    "fp-ts": "^2.9.1",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "fp-ts": "^2.9.1",
    "rxjs": "^6.6.3",
    "fp-ts-contrib": "^0.1.21",
    "graphics-ts": "^1.1.0"
  },
  "dependencies": {
    "fp-ts-contrib": "^0.1.21",
    "fp-ts-rxjs": "^0.6.11",
    "fp-ts-std": "^0.7.0",
    "graphics-ts": "^1.1.0",
    "monocle-ts": "^2.3.3",
    "rxjs": "^6.6.3"
  }
}
